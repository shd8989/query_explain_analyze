{
  "name": "query_explain_analyze",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel backend frontend",
    "backend": "cd backend && npm run start",
    "frontend": "cd frontend && npm run start",
    "setup": "npm install && npm-run-all --parallel setup-frontend setup-backend",
    "setup-frontend": "cd frontend && npm install",
    "setup-backend": "cd backend && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
